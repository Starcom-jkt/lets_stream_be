<table id="example2" class="table table-bordered table-hover table-striped" style="width: 100%">
    <thead>
      <tr>
        <th style="width: 7%">No</th>
        <th>Profile</th>
        <th>Nama Agent</th>
        <th>Username</th>
        <th style="width: 20%">Actions</th>
      </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < agent.length; i++){ %>
        <tr>
          <td><%= i + 1 %></td>
          <td>profile</td>
          <td><%= agent[i].name %></td>
          <td><%= agent[i].username %></td>
          <td>
            <form action="/agent/delete/<%= agent[i].id %>?_method=DELETE" method="POST">
              <a href="/agent/edit/<%= agent[i].id %>" class="btn btn-warning">
                <i class="fas fa-pencil-alt"></i> Edit
              </a>
              <button type="submit" class="btn btn-danger">
                <i class="fas fa-trash-alt"></i> Delete
              </button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>
  